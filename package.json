{
  "name": "jasmine-loud",
  "version": "1.0.2",
  "description": "Jasmine matchers for Loud, web accessibility testing helper",
  "keywords": [
    "jasmine",
    "jasmine-plugin",
    "jasmine-matchers",
    "loud"
  ],
  "author": "Ruslan Sagitov <rs@ruslansagitov.name>",
  "license": "MIT",
  "homepage": "https://github.com/ruslansagitov/jasmine-loud",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruslansagitov/jasmine-loud.git"
  },
  "bugs": {
    "url": "https://github.com/ruslansagitov/jasmine-loud/issues",
    "email": "rs@ruslansagitov.name"
  },
  "main": "./lib/jasmine-loud.js",
  "scripts": {
    "test": "npm run-script lint && npm run-script check-writing && npm run-script karma-test",
    "karma-test": "karma start ./test/karma.conf.js --single-run",
    "lint": "eslint .",
    "check-writing": "write-good README.md && write-good CONTRIBUTING.md --no-tooWordy",
    "coverage": "KARMA_COVERAGE=html karma start ./test/karma.conf.js --single-run"
  },
  "devDependencies": {
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "jasmine-core": "^3.5.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.0.1",
    "karma-firefox-launcher": "^1.2.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-loud": "^1.0.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "loud": "^0.9.2",
    "phantomjs-prebuilt": "^2.1.16",
    "write-good": "^1.0.2"
  },
  "engines": {
    "node": ">=12"
  }
}
